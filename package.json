{
  "name": "webpack-boilerplate",
  "version": "1.1.1",
  "description": "Sensible webpack 5 boilerplate using Babel and PostCSS.",
  "main": "index.js",
  "author": "Vinokurov Bogdan",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "prod": "cross-env NODE_ENV=deploy webpack --config config/webpack.prod-nohash.js",
    "source": "cross-env NODE_ENV=development NODE_ENV_DISK=disk webpack serve --config config/webpack.dev.js",
    "lint": "eslint --ext .js src/**/*.js",
    "prettify": "prettier --write 'src/**/*.js'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@sumotto/beautify-html-webpack-plugin": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
    "react-refresh": "^0.14.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "simple-nunjucks-loader": "^3.2.0",
    "style-loader": "^3.3.3",
    "svg-spritemap-webpack-plugin": "^4.5.0",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2",
    "vue-loader": "^17.3.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swiper": "^11.0.5",
    "vue": "^3.3.9"
  }
}
