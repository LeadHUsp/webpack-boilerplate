{
  "name": "webpack-boilerplate",
  "version": "1.1.1",
  "description": "Sensible webpack 5 boilerplate using Babel and PostCSS.",
  "main": "index.js",
  "author": "Vinokurov Bogdan",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "prod": "cross-env NODE_ENV=production webpack --config config/webpack.prod-nohash.js",
    "prod-image": "cross-env NODE_ENV=production NODE_ENV_IMAGE=optimization webpack --config config/webpack.prod-nohash.js",
    "source": "cross-env NODE_ENV=development NODE_ENV_DISK=disk webpack serve --config config/webpack.dev.js",
    "lint": "eslint --ext .js src/**/*.js",
    "prettify": "prettier --write 'src/**/*.js'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@squoosh/lib": "^0.5.3",
    "@sumotto/beautify-html-webpack-plugin": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.3.2",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "mini-css-extract-plugin": "^2.4.2",
    "nunjucks": "^3.2.3",
    "postcss": "^8.4.19",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.4.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.43.5",
    "sass-loader": "^12.2.0",
    "simple-nunjucks-loader": "^3.2.0",
    "style-loader": "^3.3.0",
    "svg-spritemap-webpack-plugin": "^4.4.0",
    "terser-webpack-plugin": "^5.3.1",
    "vue-loader": "^17.0.1",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swiper": "^9.1.0",
    "vue": "^3.2.41"
  }
}
